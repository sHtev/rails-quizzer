<h1>Your Quizzes</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th></th>
    <th></th>
  </tr>
 
  <% @quizzes.each do |quiz| %>
    <tr>
      <td><%= quiz.title %></td>
      <td><%= quiz.description %></td>
      <td><%= link_to 'Show', quiz_path(quiz) %></td>
      <td><%= link_to 'Edit', edit_quiz_path(quiz) %></td>
    </tr>
  <% end %>
</table>
